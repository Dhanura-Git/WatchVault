        <script>
            document.addEventListener('DOMContentLoaded', function () {
                var form = document.getElementById('addaddress');
        
                form.addEventListener('submit', function (event) {
                    if (!validateForm()) {
                        event.preventDefault();
                    }
                });
        
                function validateForm() {
                    const name = document.querySelector('input[name="name"]').value.trim();
                    const phone = document.querySelector('input[name="phone"]').value.trim();
                    const altphone = document.querySelector('input[name="altphone"]').value.trim();
                    const address = document.querySelector('input[name="address"]').value.trim();
                    const city = document.querySelector('input[name="city"]').value.trim();
                    const state = document.querySelector('input[name="state"]').value.trim();
                    const country = document.querySelector('input[name="country"]').value.trim();
        
                   
                    const nameError = document.getElementById('nameError');
                    const phoneError = document.getElementById('phoneError');
                    const altphoneError = document.getElementById('altphoneError');
                    const addressError = document.getElementById('addressError');
                    const cityError = document.getElementById('cityError');
                    const stateError = document.getElementById('stateError');
                    const countryError = document.getElementById('countryError');
        
                
                    clearErrorMessages();
        
                    let isValid = true;
        
               
                    if (!name) {
                        displayErrorMessage('nameError', 'Name is required.');
                        isValid = false;
                    } else {
                        const nameRegex = /^[A-Za-z\s]+$/;
                        if (!nameRegex.test(name)) {
                            displayErrorMessage('nameError', 'Invalid name format.');
                            isValid = false;
                        }
                    }
        
                    
                    if (!phone) {
                        displayErrorMessage('phoneError', 'Phone number is required.');
                        isValid = false;
                    } else {
                        const phoneRegex = /^\d{10}$/;
                        if (!phoneRegex.test(phone)) {
                            displayErrorMessage('phoneError', 'Invalid phone number format.');
                            isValid = false;
                        }
                    }
        
                   
                    if (!altphone) {
                        displayErrorMessage('altphoneError', 'Alt Phone number is required.');
                        isValid = false;
                    } else {
                        const altphoneRegex = /^\d{10}$/;
                        if (!altphoneRegex.test(altphone)) {
                            displayErrorMessage('altphoneError', 'Invalid Alt Phone number format.');
                            isValid = false;
                        }
                    }
        
                   
                    if (!address) {
                        displayErrorMessage('addressError', 'Address is required.');
                        isValid = false;
                    }
        
                 
                    if (!city) {
                        displayErrorMessage('cityError', 'City is required.');
                        isValid = false;
                    }
        
                 
                    if (!state) {
                        displayErrorMessage('stateError', 'State is required.');
                        isValid = false;
                    }

                    if (!country) {
                        displayErrorMessage('countryError', 'Country is required.');
                        isValid = false;
                    }
        
        
                    return isValid;
                }
        
                function displayErrorMessage(errorId, message) {
                    var errorElement = document.getElementById(errorId);
                    errorElement.textContent = message;
                }
        
                function clearErrorMessages() {
                    var errorElements = document.querySelectorAll('.error-message');
                    errorElements.forEach(function (element) {
                        element.textContent = '';
                    });
                }
            });
        </script>

