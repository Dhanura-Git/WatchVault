                    <div class="row">
                        <div class="col-md-9">
                            <div class="card mb-4">
                                <header class="card-header">
                                </header> <!-- card-header end// -->
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table table-hover">
                                            <thead>
                                                <tr>
                                                    <!-- <th>ID</th> -->
                                                    <th>Name</th>
                                                    <th>Email</th>
                                                    <th>Price</th>
                                                    <th>Status</th>
                                                    <th>Date</th>
                                                    <th class="text-end"> Action </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% orders.forEach(order=> {
                                                    const user = users.find(user => user._id.toString() ===
                                                    order.userId.toString());
                                                    %>
                                                    <tr>

                                                        <td>
                                                            <%= user ? user.name : 'Unknown User' %>
                                                        </td>
                                                        <td>
                                                            <%= user ? user.email : 'Unknown Email' %>
                                                        </td>
                                                        <td>â‚¹<%= order.totalPrice %>
                                                        </td>
                                                        <td><span class="badge rounded-pill alert-warning">
                                                                <%= order.status %>
                                                            </span></td>
                                                        <td>
                                                            <%= new Date(order.placed).toLocaleDateString() %>
                                                        </td>
                                                        <td class="text-end">
                                                            <a href="/admin/order-Details?id=<%=order._id%>"
                                                                class="btn btn-md rounded font-sm"> Order Details</a>

                                                        </td>
                                                    </tr>
                                                    <% }) %>
                                            </tbody>
                                        </table>
                                    </div> <!-- table-responsive //end -->
                                    <div class="pagination-area mt-30 mb-50">
                                <nav aria-label="Page navigation example">
                                    <ul class="pagination justify-content-start">

                                        <li class="page-item <%= currentpage === 1 ? 'disabled' : '' %>">
                                            <a class="page-link" href="?page=<%= currentpage - 1 %>">
                                                <i class="material-icons md-chevron_left"></i>
                                            </a>
                                        </li>

                                        <% for(let i=1; i <=totalpages; i++) { %>
                                            <li class="page-item <%= i === currentpage ? 'active' : '' %>">
                                                <a class="page-link" href="?page=<%= i %>">
                                                    <%= i %>
                                                </a>
                                            </li>
                                            <% } %>

                                                <li
                                                    class="page-item <%= currentpage === totalpages ? 'disabled' : '' %>">
                                                    <a class="page-link" href="?page=<%= currentpage + 1 %>">
                                                        <i class="material-icons md-chevron_right"></i>
                                                    </a>
                                                </li>

                                    </ul>
                                </nav>
                            </div>
                                </div> <!-- card-body end// -->
                            </div> <!-- card end// -->
